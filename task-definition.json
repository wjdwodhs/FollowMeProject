{
  "family": "followme-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::516268691817:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "followme-container",
      "image": "<IMAGE_URI>",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}

